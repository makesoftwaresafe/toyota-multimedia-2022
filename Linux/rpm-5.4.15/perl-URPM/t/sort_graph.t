sub create_random {
    my ($list_unsorted) = @_;
    my %requires;

    foreach my $i (@$list_unsorted) {
	my $nb = int(rand(1) * 2.8);
	push @{$requires{$i}}, grep { $_ != $i } uniq(map { int(rand @$list_unsorted) } 1 .. $nb);
    }
    \%requires;
}
sub uniq { my %l; $l{$_} = 1 foreach @_; grep { delete $l{$_} } @_ }
