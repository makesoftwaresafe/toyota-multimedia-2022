[Unit]
Description=GENIVI AudioManager
Requires=dbus.service
After=dbus.service

[Service]
Type=notify
Environment=RTS_LOGLEVEL=4
ExecStart=/usr/bin/AudioManager -T -c /usr/lib/audiomanager/control/libPluginControlInterfaceSmoke.so
Restart=always
TimeoutSec=2
WatchdogSec=5

[Install]
WantedBy=multi-user.target

